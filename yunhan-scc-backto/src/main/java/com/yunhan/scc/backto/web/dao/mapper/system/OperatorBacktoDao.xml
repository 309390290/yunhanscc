<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yunhan.scc.backto.web.dao.mapper.system.OperatorBacktoDao">



	<!-- 用户修改密码 -->
	<update id="updatePassword" >
		update T_OPERATOR t set t.SO_PASSWORD=#{newPassword} where	 t.SO_ID = #{userId}
	</update>
	
</mapper>